<h1>Editing score</h1>

<%= render "form", score: @score %>

<br>

<div>
  <%= link_to "Show this score", @score %> |

  <% if(session[:from_edit_score] == 'true') %>
    <%= link_to "Back to scores", '/students/' + @score.student_id.to_s + '/edit_score' %>
  <% else %>
    <%= link_to "Back to scores", scores_path %>
  <% end %>
  
</div>
